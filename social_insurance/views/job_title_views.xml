<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

    <record id="job_title_view_tree" model="ir.ui.view">
        <field name="name">job.title.view.tree</field>
        <field name="model">job.title</field>
        <field name="arch" type="xml">
            <list editable="bottom">
                <field name="name"/>
            </list>
        </field>
    </record>

    <record id="job_title_action" model="ir.actions.act_window">
        <field name="name">Job Title</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">job.title</field>
        <field name="view_mode">list</field>
    </record>


    <menuitem id="menu_job_title" name="Job Title" parent="social_insurance.menu_social_insurance_root"
              action="social_insurance.job_title_action"
              sequence="20"/>


</odoo>