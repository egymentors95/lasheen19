<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

    <record id="social_category_view_tree" model="ir.ui.view">
        <field name="name">social.category.view.tree</field>
        <field name="model">social.category</field>
        <field name="arch" type="xml">
            <list editable="bottom">
                <field name="name"/>
            </list>
        </field>
    </record>

    <record id="social_category_action" model="ir.actions.act_window">
        <field name="name">Social Category</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">social.category</field>
        <field name="view_mode">tree</field>
    </record>


    <menuitem id="menu_social_category" name="Social Category" parent="social_insurance.menu_social_insurance_root"
              action="social_insurance.social_category_action"
              sequence="30"/>


</odoo>