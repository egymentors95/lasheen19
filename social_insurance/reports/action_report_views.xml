<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

    <record id="paperformat_social_insurance" model="report.paperformat">
        <field name="name">Social Insurance</field>
        <field name="default" eval="True"/>
        <field name="format">custom</field>
        <field name="page_height">297</field>
        <field name="page_width">210</field>
        <field name="orientation">Landscape</field>
        <field name="margin_top">28</field>
        <field name="margin_bottom">15</field>
        <field name="margin_left">3</field>
        <field name="margin_right">3</field>
        <field name="header_line" eval="False"/>
        <field name="header_spacing">25</field>
        <field name="dpi">90</field>
    </record>


    <record id="social_insurance_wizard_pdf_report" model="ir.actions.report">
        <field name="name">Social Insurance (PDF)</field>
        <field name="model">social.insurance.wizard</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">social_insurance.social_insurance_template</field>
        <field name="print_report_name">'Social Insurance PDF'</field>
        <field name="paperformat_id" ref="social_insurance.paperformat_social_insurance"/>

    </record>


</odoo>